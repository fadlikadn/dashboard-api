{
   "name": "patient-dashboard-api",
   "version": "0.0.1",
   "description": "Patient Dashboard API",
   "type": "commonjs",
   "devDependencies": {
      "@types/morgan": "^1.9.9",
      "@types/node": "^16.11.10",
      "@types/swagger-ui-express": "^4.1.7",
      "concurrently": "^9.1.0",
      "nodemon": "^3.1.7",
      "ts-node": "^10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/express": "^5.0.0",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/memory-cache": "^0.2.6",
      "bcrypt": "^5.1.1",
      "class-validator": "^0.14.1",
      "dotenv": "^16.4.6",
      "express": "^4.21.1",
      "jsonwebtoken": "^9.0.2",
      "memory-cache": "^0.2.0",
      "morgan": "^1.10.0",
      "pg": "^8.13.1",
      "reflect-metadata": "^0.1.14",
      "swagger-ui-express": "^5.0.1",
      "tsoa": "^6.5.1",
      "typeorm": "^0.3.20"
   },
   "scripts": {
      "start": "node build/index.js",
      "predev": "npm run swagger",
      "prebuild": "npm run swagger",
      "build": "tsc",
      "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec \"",
      "swagger": "tsoa spec"
   },
   "nodemonConfig": {
      "watch": [
         "src"
      ],
      "ext": "ts",
      "exec": "ts-node src/index.ts"
   }
}
